{
    "name": "the-startup-cto-theme",
    "description": "Theme for Ghost which use 'Ghost in The Shell' animation series palette colors",
    "version": "3.0.0",
    "engines": {
        "ghost": ">=3.0.0",
        "ghost-api": "v3"
    },
    "license": "MIT",
    "screenshots": {
        "desktop": "assets/screenshot-desktop.jpg",
        "mobile": "assets/screenshot-mobile.jpg"
    },
    "scripts": {
        "start": "cd ../../.. && ghost start",
        "watch": "gulp watch",
        "zip": "gulp release",
        "test": "gscan ."
    },
    "author": {
        "name": "Daniel Bartholomae",
        "email": "daniel@bartholomae.name",
        "url": "https://the-startup-cto.net"
    },
    "gpm": {
        "type": "theme",
        "categories": [
            "Minimal",
            "Personal Blogs"
        ]
    },
    "keywords": [
        "ghost",
        "theme",
        "ghost-theme"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dbartholomae/the-startup-cto-theme.git"
    },
    "devDependencies": {
        "browser-sync": "^2.26.3",
        "gscan": "^3.5.6",
        "gulp": "^4.0.0",
        "gulp-autoprefixer": "^6.0.0",
        "gulp-sass": "^4.0.2",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-zip": "^4.2.0"
    },
    "config": {
        "posts_per_page": 5
    }
}
